tree-select[tree-list] li .normal{
	padding: 1px 10px 0px 12px;
  	background-repeat: no-repeat;
	background-image: url(black/img/tree-iamges/file.png);
	background-position-x: center;
}
tree-select[tree-list] li .expanded{
	padding: 1px 10px 0px 12px;
  	background-repeat: no-repeat;
	background-image: url(black/img/tree-iamges/folder.png);
	background-position-x: center;
}
tree-select[tree-list] li .collapsed{
	padding: 1px 10px 0px 12px;
  	background-repeat: no-repeat;
	background-image: url(black/img/tree-iamges/folder-closed.png);
	background-position-x: center;
}

tree-select[tree-list] li .selected {
  background-color: #328ee3;
  color:#fff;
  padding: 1px 5px;
}

