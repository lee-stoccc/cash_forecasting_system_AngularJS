@CHARSET "UTF-8";

.tree-select{
	position: relative;
}

.tree-select>span {
	position: absolute;
	top: 6px;
	right: 4px;
	width: 16px;
    height: 16px;
	color:transparent;
	background-image: url(black/img/tree-iamges/icon_more.png);
	background-repeat:no-repeat;
 	background-position:98%;
}
.tree-select>div>p{
	left:240px !important;
}
.tree-select>tree-select{
	position: absolute;
	width: 96%;
	background-color: #fff;
	display: none;
	z-index: 9999;
    border: 1px solid #485058;
    box-shadow: 0 5px 15px rgba(0, 0, 0, .3);
    padding:0px;
    left:0px;
    top:26px;
}
/*树型*/
.tree-bg{
	background-color: #fff;
    padding: 10px;
    max-height: 520px;
    overflow-y: auto;
}
.configuration-nav-tree{
	max-height: 330px;
    overflow-y: auto;
    padding-top:0px;
    background-color: #fff;
    width:100%;
}

::-webkit-scrollbar {/*滚动条整体样式*/
    width: 14px;     /*高宽分别对应横竖滚动条的尺寸*/
    height: 14px;
 }
 
 ::-webkit-scrollbar-thumb {/*滚动条里面小方块*/
      border-radius: 0px;
      -webkit-box-shadow: inset 0 0 5px rgba(0,0,0,0.1);
      background: #ccc;
}
::-webkit-scrollbar-track {/*滚动条里面轨道*/
      -webkit-box-shadow: inset 0 0 0 rgba(0,0,0,0.1);
      border-radius: 0;
      background: #f2f2f2;
}

::-webkit-scrollbar-button:vertical:start{
	width:16px;
	height:16px;
	background:#fff;
	background-image: url(black/img/tree-iamges/icon_up.png);
	background-repeat:no-repeat;
 	background-position:center;
}
::-webkit-scrollbar-button:vertical:end{
	width:16px;
	height:16px;
	background:#fff;
	background-image: url(black/img/tree-iamges/icon_down.png);
	background-repeat:no-repeat;
 	background-position:center;
}

tree-select[tree-view]{
	color:#000;
	font-size:1.2rem;
	/* position: relative; */
  	padding: 0 0 0 10px;
  	line-height: 30px;
  	
  	-moz-user-select: -moz-none;
    -khtml-user-select: none;
	-webkit-user-select: none;
	-ms-user-select: none;
	user-select: none;

    /* default */
    font-family: Tahoma;
    font-size:13px;
    /* color: #555; */
    text-decoration: none;
}
tree-select[tree-list] ul {
  margin: 0;
  padding: 0;
  list-style: none;
  border: none;
}

tree-select[tree-list] li {
  position: relative;
  padding: 0 0 0 20px;
  line-height: 23px;
}
tree-select[tree-list] li:before{
	display: inline-block;
    content: "";
    position: absolute;
    top: -12px;
    left: 0;
    width: 25px;
    height: 25px;
    border: 1px dotted #fff;
    border-style: none none solid solid;
    border-width: 1px; 
}

tree-select[tree-list] li:not(:last-child):after {  
    position: absolute;  
    content: '';  
    top: -12px;  
    left: 0;  
    bottom:0;  
    border: 1px dotted #fff; 
    border-style: none none none solid;  
    border-width: 1px;  
} 

