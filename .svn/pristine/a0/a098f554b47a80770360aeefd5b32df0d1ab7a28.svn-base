body{
	font-size: 13px;
	color:#333;
	padding:0px;
	margin:0px;
	font-family: "microsoft yahei";
	background: #eeeeee;
}
.leftpanel{
	min-height: 100%;
    width: 100px;
    z-index: 999;
    position: absolute;
    background:#444;
    left:0px;
    padding:0px;
}
.nav.side-menu>li {
    position: relative;
    display: block;
    cursor: pointer;
    padding:7px 0px;

}
.nav.side-menu li a {
    font-weight: 400;
    padding: 10px 5px 10px 10px;
    color: #CACACA;
}
.nav.side-menu li span,.second-menu>li>a{
	font-size: 12px;
}
.nav.side-menu li a i {
    font-size: 12px!important;
    text-align: center;
    margin-bottom: 5px;
}
.fa{
	display: inline-block;
	font-weight: 400;
	padding-right:7px;
}
.nav.child_menu li {
    padding: 0 10px;
}
.nav.child_menu li a {
    text-align: left!important;
}
.side-menu>.active>a>i,.side-menu>.active>a>span{
	color:#000;
}
.nav-parent:hover>a>i,.nav-parent:hover>a>span{
	color: #fff;
}
.side-menu>.active>a{
	background: #fff;
}
.nav-parent:hover>a{
	background: #666 !important;
}
.side-menu>.active:hover>a{
	background: #fff !important;
}
.side-menu>.active:hover>a>i,.side-menu>.active:hover>a>span{
	color: #000 !important;
}
.second-menu{
	position: fixed;
    top: 0px;
    bottom: 0;
    left: 100px;
    width: 110px;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    background: #fff;
    border-right:1px solid #e5e5e5;
}
.second-menu>li{
		height:50px;
		padding:5px 7px;
}
.second-menu>li>a{
	color: #666 !important;
}
.menu-title{
	border-bottom:1px solid #e9e9e9;
}
.menu-title a{
    font-size: 14px !important;
}
.nav.second-menu li a{
	text-align: center;
    padding-top: 10px;
    padding-bottom: 10px;
}
.se-me-active>a{
	background-color: #f3f3f3;
}
.mainpanel {
    margin-left: 210px;
}
.headermenu{
     list-style: none;
    margin: 0;
    padding: 0;
}
.headermenu>.header-menu-li{
	display: inline;
	padding:20px;
}
.headermenu>.header-menu-li>a{
    text-decoration: none;
    height: 50px;
    line-height: 48px;
    min-width: 0;
    border: none;
    background: transparent;
    font-size: 14px;
    color: #666;
    text-align: center;
    display: inline-block;
}
.header-left{
	display: inline-block;
}
.header-right{
	display: inline-block;
	float: right;
}
.nav-tab>li:hover{
	cursor: pointer;
}
.nav-tab>li:hover a{
		color:#0099fc;
}
.fa-dele:hover{
	color:#0099fc;
}
.block{
	background: #fff;
}
.headermenu.nav-tab>.active>a{
	border-bottom:2px solid #0099fc;
}
.fa-dele{
	position: relative;
    left: 12px;
    color: #7e7e7e;
}
.btn-nav{
	height:50px;
	background: none;
	border-left:1px solid #e9e9e9;
	border-right:1px solid #e9e9e9;
	border-radius: 0px;
	border-top:none;
	border-bottom:0px;
}
.btn-nav>.badge{
	background:#D9534F ;
}
.btn-nav:hover{
	background: #f3f3f3;
}
.re-dropdown-menu{
	width: 109px;
    right: 0px;
    left:auto;
}
.re-dropdown-menu>li>a{
	height:auto;
    line-height: 2;
    color: #333;
}
.header-right>.headermenu>.header-menu-li{
	padding:0px;
}
.header-menu-li>.btn-group>button{
	margin-left:-5px !important;
}
.btn-group>button>img{
	vertical-align: middle;
    margin-right: 5px;
    width: 26px;
    -moz-border-radius: 50px;
    -webkit-border-radius: 50px;
    border-radius: 50px;
}
.btn{
	outline: none !important;
}
.header-menu-li>.btn-group>.btn:active{
	color: #333;
    background-color: #d4d4d4;
    border-color: #8c8c8c;
}
.contentpanel{
	margin:10px;
	background: #fff;
}
.re-form-control{
	height:30px;
}
.form-group{
	margin-right:10px;
	margin-bottom:10px !important;
}
.btn{
	padding: 4px 12px;
}
.btn-primary{
	background: #59f;
	color: #fff;
	border-color: #4185eb;
}
.search-block>.form-inline>.btn-primary{
		margin-right: 10px;
    margin-bottom: 10px;
}
.btn-primary:hover{
	background: #48a6f6;
}
.btn-primary:active,.btn-primary:active:hover{
	background: #59f;
}
.time-input{
	width:250px !important;
}
.input-group-addon{
	border-left:none;
}
.tool-bar{
	padding:10px;
}
.table-title>th{
	background: #eee;
}
.num{
	color: #337ab7 !important;
	font-weight: bold;
}
.re-table>tbody>>tr>td>a:hover{
  cursor: pointer;
}
.re-pagination{
	margin:0px;
}
.record{
	display: inline-block;
    line-height: 36px;
}
.re-table{
	margin-bottom:10px;
}
.grid-pager{
	padding:5px 7px 10px 7px;
}
.table-content{
	height:480px;
	overflow-y:scroll;
}
.footer>p{
	text-align: center;
	color: #ADADAD;
}
.login-form{
	margin: 20px 0;
    position: relative;
    background: #fff;
    padding: 20px 40px;
}
.login-dialog{
	width:350px;
	margin: 10% auto;
}
.login-form{
	    box-shadow: 0 5px 15px rgba(0,0,0,.5);
}
.login-form h1{
	letter-spacing: -.05em;
    line-height: 20px;
    margin: 10px 0 30px;
    color: #2196f3;
    font-size: 25px;
    text-align: center;
}
.login-form input{
	border: 1px solid #dde3ec;
    color: #666;
    margin: 0 0 20px;
    width: 100%;
    height: 40px;
    background: #dde3ec;
    box-shadow: none;
}
.login-dialog form input[type=email]:focus, .login-dialog form input[type=password]:focus, .login-dialog form input[type=text]:focus {
    background-color: #fff;
    border: 1px solid #2196f3;
    outline: 0;
}
.login-btn,.login-btn:hover{
	font-size: 12px;
    margin: 10px 15px 0 0;
    background: #2196f3;
    color: #fff;
    border-color: #2196f3;
    padding: 7px 20px;
}
.copyright{
	text-align: center;
}
.copyright p{
	color: #666;
}
.validate-input{
	width:68% !important;
	float: left;
}
.vali-code{
	width: 80px;
    display: block;
    display: inline-block;
    margin-left: 5px;
        background: #eee;
    padding-top: 5px;
    padding-bottom: 5px;
}
.vali-code img{
	width:100%
}
.mainpane2{
	margin-left:100px;
}

.re-modal{
	margin-top: 10%;
}
.window-form-group{
  display: block !important;
}
.form--lable label{
	width: 100px;
    text-align: right;
}
.form--lable input,select{
	width:250px !important;
}
.re-popover-content{
	    padding: 5px 10px;
}
