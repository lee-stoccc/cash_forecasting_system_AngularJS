.body-container{
    clear: both;
    margin:0;
    padding: 0;
    height:750px
    background-color: white;
    margin-bottom: 50px;
}

.menu-management-container{
    height: 600px;
    position: relative;
}

.menu-management-container-left{
    display:table-row;
    height: 100%;
    float: left;
}

.menu-management-nav-cell{
    width: 200px;
    max-height: 650px;
    overflow: scroll;
    padding-top: 10px;
}

.menu-management-container-right{
    position: absolute;
    top: 0;
    left: 210px;
    right: 0;
	bottom:0; 
   	border: 1px solid #ccc;
}

.container-header{
    height: 40px;
    width: auto;
    line-height: 40px;
    font-size: 15px;
    text-align: left;
    padding-left: 10px;
    background: #eee;
    font-weight: 700;
}

.table-th-border-5{
    width: 5%;
    text-align: center;
    border:1px solid #ccc;
}

.table-th-border-15{
    width: 15%;
    text-align: center;
    border:1px solid #ccc;
}

.table-th-border-20{
    width: 20%;
    text-align: center;
    border:1px solid #ccc;
}

.table-th-border-225{
    width: 22.5%;
    text-align: center;
    border:1px solid #ccc;
}

.menu-table{
    height: 400px;
}

.pager-panel{
    position: absolute;
    width: auto;
    bottom: 10px;
    right:20px;
    left: 20px;
}

.form-btn-css{
    margin-top: 8px;
    margin-left: 5px;
}

.form-container{
    height: 540px;
    padding: 0;
    border-top:  1px solid #ccc;
    border-bottom: 1px solid #ccc;
}

.form-container-item {
    width: 46%;
    height: 510px;
    margin: 10px 2%;
    float: left;
    border: 1px solid #ccc;
}
